@model Videoo.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<h5>Membership Type: @Model.MembershipType.Name</h5>
@if (Model.Birthday != null)
{
<h5>Birthday: @Model.Birthday.Value.ToString("d MMM yyy")</h5>
}
<h5>The customer is @(Model.IsSubscribedToNewsletter? " " : "not") subscribed to the newsletter.</h5>

